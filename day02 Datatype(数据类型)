package d;

public class datatype {  // 是数据类型
    public static void main(String[] args) {
/*        基本数据类型
        整数  byte(取值范围 -128到127) short int long  取值范围不同
        浮点数（小数）  double  float
         字符 char
         布尔类型 bool     */
    // long 类型变量要在后面加一个l（无论大小写）
        long a=53523L;
        System.out.println(a);
    // float 类型变量要在后面加一个f（无论大小写）
       float b=4545.2323F;
       System.out.println(b);
        System.out.println("RNG");
    }
}
