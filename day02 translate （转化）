package f;

public class translate {  //转换
    public static void main(String[] args){
/*        // 数据类型不一样不可以一起运算（int 和 double 不能一起运算）  需要转换
           取值范围  byte<short<int<long<float<double                  */
       /*                          隐式转换（自动类型提升）
         把一个取值范围小的数值转换取值范围大的数据  */
        int a=10;
        double b=a;
        System.out.println(b);  //输出为10.0
        // 提升规则 1 取值范围小的和取值范围大的运算时 要把取值范围小的提升为大的进行运算
             int c=10;
             double d=10.0;
             double e=c+d;  // 这里要用double
        System.out.println(e);
        // 提升规则 2  byte short char三种类型在运算时需要先转换为int才能运算
              byte f=1;
              short g=2;
              int h=f+g;  //这里的是int
        System.out.println(h);

      //                                 强制转换
      // 把取值范围大的数据给取值范围小的 需要强制转换
         double i=12.333;
         int j=(int)i;          // 格式
         System.out.println(j);
         /*  int l=128;
             byte o=(byte)l;             错误的 l的取值比byte 的取值范围大了
         System.out.println(o);*/
        System.out.println("RNG");
    }

}
